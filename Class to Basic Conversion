#include<iostream>
using namespace std;


class Number{
    public:
    
     int val;
     
     Number(int val)
     {
         this->val=val;
     }
    
    
     operator int()
     {
         return val;
     }
    
};


int main()
{
   Number n(10);
   
   int x=n;
   
   cout<<x<<endl;
    
    
    return 0;
}